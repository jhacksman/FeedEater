generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Example private persistence for the module.
// FeedEater convention: each module owns its own Postgres schema: mod_<moduleName>.
model ExampleRawEvent {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  payload   Json

  @@schema("mod_example")
}


